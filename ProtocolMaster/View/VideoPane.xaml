<Page x:Class="ProtocolMaster.View.VideoPane"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProtocolMaster.View"
      mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="900"
      Title="VideoPane">

    <DockPanel >
        <!--Camera/Video Toolbar-->
        <Menu DockPanel.Dock="Top">
                <TextBlock Style="{StaticResource ToolHeader}">Video Feed</TextBlock>
        </Menu>
        <!--Camera/Video Content-->
        <TextBlock>I am a video/camera of a booth and a rat!</TextBlock>
    </DockPanel>
</Page>
