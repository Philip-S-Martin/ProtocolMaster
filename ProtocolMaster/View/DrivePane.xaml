
    <Page x:Class="ProtocolMaster.View.DrivePane"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProtocolMaster.View"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="300"
      Title="Page1">
    <Grid>
        <DockPanel Background="White">
            <!--Google Drive Toolbar-->
            <ToolBarTray DockPanel.Dock="Top">
                <ToolBar>
                    <TextBlock Style="{StaticResource ToolHeader}">Google Drive</TextBlock>
                    <Button Content="Sign-In" Name="AccountButton" Click="Account_Click"/>
                    <Button Content="Refresh" Name="RefreshButton" Click="Refresh_Click"/>
                    <Button Content="New Folder" Name="FolderButton" Click="Folder_Click"/>
                    <Button IsEnabled="False">Poop Button!</Button>
                </ToolBar>
            </ToolBarTray>
            <!--Google Drive Treeview-->
            <TreeView  Name="FileTree" BorderThickness="0" Background="White">
                <TreeViewItem Header="McIntyre">
                    <TreeViewItem Header="VNS AFC Volume Exp"/>
                </TreeViewItem>
            </TreeView>
        </DockPanel>
    </Grid>
</Page>
